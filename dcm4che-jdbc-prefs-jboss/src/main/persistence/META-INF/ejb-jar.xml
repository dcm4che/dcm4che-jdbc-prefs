  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
  version="3.1">
  <module-name>jdbc-prefs</module-name>
  <enterprise-beans>
    <session>
      <ejb-name>QueryPreferencesBean</ejb-name>
      <ejb-class>org.dcm4che.jdbc.prefs.QueryPreferencesBean</ejb-class>
    </session>
  </enterprise-beans>
</ejb-jar>
