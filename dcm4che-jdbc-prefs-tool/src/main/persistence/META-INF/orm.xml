<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
  version="2.0">

  <entity class="org.dcm4che.jdbc.prefs.entity.Node">
    <table name="node" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="${generated-value.strategy}" />
      </id>
      <basic name="encodedAttributes" optional="false">
        <column name="pat_attrs" column-definition="blob(262144)" />
      </basic>
    </attributes>
  </entity>

  <entity class="org.dcm4che.jdbc.prefs.entity.Attribute">
    <table name="attribute" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="${generated-value.strategy}" />
      </id>
    </attributes>
  </entity>

</entity-mappings>
